ul
    width 100%
    display flex
    justify-content center
    align-content center
    align-items center
    padding 0
    margin-top 2%

li
    width 9%
    list-style none
    text-align center
    font-size 0.9em
    font-weight 100

.search-ico 
    width 25%

.bag-ico 
    width 29%
    font-weight bold

li:first-child,
li:last-child
    width 6%

.logo
    padding 0 2.5%
    width 13%
    img
        width 90%

.bag-number
    width 17px
    height 17px
    border-radius 50%
    background-color #da2394
    position relative
    color #fff
    font-size 0.7em
    display flex
    justify-content center
    align-content center
    align-items center
    float left
    left 63%
    top 7px